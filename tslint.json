{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "jsx-no-bind": false,
    "jsx-no-multiline-js": false,
    "prettier": true
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}